{
  "compilerOptions": {
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "baseUrl": ".",
    "outDir": "lib",
    "incremental": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strictPropertyInitialization": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "lib": ["dom", "ES2020"]
  }
}
