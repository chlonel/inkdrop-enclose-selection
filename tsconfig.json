{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "outDir": "lib",
    "incremental": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "strictPropertyInitialization": false,
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "lib": [
      "dom",
      "ES2021"
    ]
  }
}
