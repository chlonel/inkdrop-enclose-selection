{
  ".CodeMirror.has-selections": {
    "'": "enclose-selection:single-quotes",
    "\"": "enclose-selection:double-quotes",
    "`": "enclose-selection:backticks",
    "(": "enclose-selection:parenthesis",
    "{": "enclose-selection:curly-brackets",
    "[": "enclose-selection:square-brackets",
    "<": "enclose-selection:angle-brackets"
  }
}
